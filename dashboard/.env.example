# Dashboard Configuration
PORT=8080
NODE_ENV=development

# Discord OAuth2
DISCORD_CLIENT_ID=your_bot_client_id
DISCORD_CLIENT_SECRET=your_bot_client_secret
DISCORD_CALLBACK_URL=http://localhost:8080/auth/discord/callback

# Session Secret
SESSION_SECRET=your_random_session_secret_here_change_this

# MongoDB (same as bot)
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# Bot Configuration Path
BOT_CONFIG_PATH=../config.js
BOT_ENV_PATH=../.env

# Admin User IDs (comma-separated Discord user IDs who can access dashboard)
ADMIN_IDS=123456789012345678,987654321098765432

# Dashboard URL
DASHBOARD_URL=http://localhost:8080
